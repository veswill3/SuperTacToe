<div class="container-fluid">
  <div class="row">
    <%- partial('header') %>
  </div>

  <div class="row">
    <div class="col-md-2">
    <nav class="navbar navbar-default">
      Your turn
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#game-list">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <div ng-controller="GameListController as gameList">
        <ul class="nav nav-pills nav-stacked collapse navbar-collapse" id="game-list">
          <li role="presentation" ng-repeat="game in gameList.games">
            <a href="#">{{game.name}}</a>
          </li>
        </ul>
      </div>
    </nav>
    </div>
    <div class="col-md-7">
      <%- partial('superttt') %>
    </div>
    <div class="col-md-3">
      Chat window here
    </div>
  </div>

  <div class="row">
    <div class="col-md-12">
      <%- partial('footer') %>
    </div>
  </div>
</div>
